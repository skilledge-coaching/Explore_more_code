    "import re\n",
    "\n",
    "string = \"C O D E WOR LD\"\n",
    "spaces = re.compile(r'\\s+')\n",
    "result = re.sub(spaces, \"\", string)\n",
    "print(f\"The revised result is: {result}\")"
   ]
